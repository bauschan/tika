{
	
	"name": "apachesolrfortypo3/tika",
	"type": "typo3-cms-extension",
	"description": "Apache Tika for TYPO3",
	"version": "2.0.1",
	"keywords": ["typo3", "cms", "tika", "meta data", "language", "text", "extraction"],
	"homepage": "http://www.typo3-solr.com",
	"license": "GPL-2.0+",
	"source": {
		"type": "git",
		"url": "git://git.typo3.org/TYPO3CMS/Extensions/tika.git",
		"reference": "master"
	},
	"authors": [
		{
			"name": "Ingo Renner",
			"email": "ingo@typo3.org",
			"role": "Lead Developer"
		}
	],
	"support": {
		"email": "typo3-project-solr@lists.typo3.org",
		"issues": "https://forge.typo3.org/projects/extension-tika/issues",
		"forum": "http://forum.typo3.org/index.php/f/53/",
		"wiki": "https://forge.typo3.org/projects/extension-tika/wiki",
		"source": "https://forge.typo3.org/projects/extension-tika/repository"
	},
	"repositories": [
		{
			"type": "composer",
			"url": "http://composer.typo3.org/"
		}
	],
	"require": {
		"php": ">5.3.7,~5.5.0",
		"typo3/cms": "~6.2.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.5.0"
	},
	"autoload": {
		"psr-4": {
			"ApacheSolrForTypo3\\Tika\\": "Classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"ApacheSolrForTypo3\\Tika\\Tests\\": "Tests/"
		}
	}
}
